{
  "name": "tpp-react-sdk",
  "version": "0.0.1",
  "description": "Tranzakt Payment Gateway Reactjs SDK.",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "build:esm": "tsc --module ES2020 --outDir dist/esm",
    "build:cjs": "tsc --module CommonJS --outDir dist/cjs",
    "build": "npm run build:esm && npm run build:cjs",
    "test": "jest"
  },
  "keywords": [
    "Tranzakt, TPP, Payment Gateway"
  ],
  "author": "Tranzakt Developers",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^22.1.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.4",
    "typescript": "^5.5.4"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "default": "./dist/esm/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tranzakt-Financial-Services-Limited/Tranzakt-Payment-Gateway-React-SDK.git"
  },
  "homepage": "https://github.com/Tranzakt-Financial-Services-Limited/Tranzakt-Payment-Gateway-React-SDK#readme",
  "files": [
    "dist/"
  ]
}
